this.flight = function(){
  height = 93;
  /*Runner.keycodes = {RESTART:{}, JUMP:{}, DUCK:{}}*/;
  this.Keycodes = {'38':function(){height -= 6},'40':function(){height += 6}};
  document.addEventListener('keydown', function(k){hacks.flight.Keycodes[String(k.keyCode)]()});
  setInterval(function(){Runner.instance_.tRex.yPos = height},1);
  Runner.instance_.tRex.startJump = function(){}
}
this.immortality = function(){
  Runner.instance_.gameOver = function(){}
}


